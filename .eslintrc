{
    "env": {
        "es2021": true
    },
    "overrides": [
        {
            "env": {
                "node": true
            },
            "files": [
                ".eslintrc.{js,cjs}"
            ],
            "parserOptions": {
                "sourceType": "script"
            }
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-new-native-nonconstructor": [
            "error"
        ],
        "camelcase": [
            "warn"
        ],
        "class-methods-use-this": [
            "warn"
        ],
        "complexity": [
            "warn",
            5
        ],
        "consistent-return": [
            "error"
        ],
        "curly": [
            "error",
            "multi"
        ],
        "default-case": [
            "error"
        ],
        "default-case-last": [
            "error"
        ],
        "default-param-last": [
            "error"
        ],
        "dot-notation": [
            "warn"
        ],
        "eqeqeq": [
            "error"
        ],
        "max-depth": [
            "warn",
            4
        ],
        "max-params": [
            "error",
            4
        ],
        "new-cap": [
            "error"
        ],
        "no-invalid-this": [
            "error"
        ],
        "no-lonely-if": [
            "warn"
        ],
        "no-mixed-operators": [
            "warn"
        ],
        "no-param-reassign": [
            "error"
        ],
        "no-sequences": [
            "error"
        ],
        "no-shadow": [
            "error"
        ],
        "no-unneeded-ternary": [
            "warn"
        ],
        "no-unused-expressions": [
            "warn"
        ],
        "no-useless-return": [
            "warn"
        ],
        "no-var": [
            "error"
        ],
        "prefer-rest-params": [
            "warn"
        ],
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "key-spacing": [
            "warn"
        ],
        "max-len": [
            "error",
            100
        ]
    }
}
